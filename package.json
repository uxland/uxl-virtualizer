  "repository": {
    "type": "git",
    "url": "https://github.com/uxland/uxl-virtualizer.git"
  },
  "bugs": {
    "url": "https://github.com/uxland/uxl-virtualizer/issues"
  },
  "author": "UXLand <dev@uxland.es> (http://www.uxland.es)",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "lib/index.d.ts",
  "keywords": [],
  "devDependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/preset-env": "^7.7.6",
    "@olavoparno/jest-badges-readme": "^1.3.6",
    "@types/jest": "^24.0.23",
    "@types/jest-when": "^2.4.0",
    "@types/ramda": "^0.26.36",
    "@uxland/generator-lit-component": "^1.0.2",
    "@webcomponents/custom-elements": "^1.3.1",
    "@webcomponents/webcomponentsjs": "^2.4.0",
    "circular-dependency-plugin": "^5.2.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.0",
    "cpx": "^1.5.0",
    "css-loader": "^3.3.0",
    "dotenv-webpack": "^1.7.0",
    "ejs-loader": "^0.3.5",
    "file-loader": "^4.3.0",
    "html-webpack-exclude-assets-plugin": "0.0.7",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.1.0",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jest": "^24.5.0",
    "jest-when": "^2.5.0",
    "jsdom": "^16.5.0",
    "mutation-observer": "^1.0.3",
    "node-sass": "^4.12.0",
    "postcss": "^7.0.24",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-mixins": "^6.2.2",
    "precss": "^4.0.0",
    "prompt": "^1.0.0",
    "prompts": "^2.2.1",
    "ramda": "^0.26.1",
    "rimraf": "^3.0.0",
    "sass-loader": "^8.0.0",
    "script-ext-html-webpack-plugin": "^2.1.4",
    "semantic-release": "^16.0.0-beta.45",
    "travis-deploy-once": "^5.0.7",
    "ts-jest": "^24.2.0",
    "ts-loader": "^6.2.0",
    "tslib": "^1.10.0",
    "typescript": "^3.8.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.2",
    "webpack-log": "^3.0.1",
    "webpack-merge": "^4.2.2",
    "yo": "^3.1.1"
  },
  "dependencies": {
    "@polymer/polymer": "^3.3.1",
    "@uxland/uxl-utilities": "^3.3.10",
    "lit-element": "^2.2.1",
    "lit-html": "^1.1.2"
  },
  "scripts": {
    "build:es": "tsc -p tsconfig.build.json --outDir es  ",
    "build:cjs": "tsc -p tsconfig.build.json --outDir lib --module commonjs --declaration",
    "build": "npm run build:es && npm run build:cjs",
    "dist": "npm run dist:copy-resources:es && npm run dist:copy-resources:lib && npm run build",
    "dist:copy-resources:es": "cpx \"src/**/*.{gif,png,jpg,jpeg,ico,pdf,svg,scss,d.ts,css,ttf,woff,woff2,css.d.ts}\" ./es",
    "dist:copy-resources:lib": "cpx \"src/**/*.{gif,png,jpg,jpeg,ico,pdf,svg,scss,d.ts,css,ttf,woff,woff2,css.d.ts}\" ./lib",
    "readme:coverage": "node_modules/.bin/jest-badges-readme",
    "semantic-release": "semantic-release",
    "semantic-release:local": "npm run dist && semantic-release --no-ci --debug",
    "postsemantic-release:local": "git checkout -- package*",
    "tdd": "jest --watch --collect-coverage",
    "test:ui:watch": "nodemon --watch test --watch src -x \"polymer test --config-file wct.config.json\"",
    "test:ui": "polymer test --config-file wct.config.json",
    "test:unit": "jest --collect-coverage",
    "travis-deploy-once": "travis-deploy-once",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "serve": "webpack-dev-server --hot --inline --progress",
    "yo": "yo",
    "create:component": "yo @uxland/lit-component"
  },
  "directories": {
    "test": "test"
  },
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run readme:coverage && git add ."
    }
  }
}
